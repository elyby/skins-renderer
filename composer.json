{
    "name": "elyby/skins-renderer-server",
    "type": "project",
    "license": "Apache 2.0",
    "authors": [
        {
            "name": "ErickSkrauch",
            "email": "erickskrauch@yandex.ru"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "require": {
        "php": "^8.3",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "erickskrauch/skins-renderer-2d": "dev-master",
        "guzzlehttp/guzzle": "^7",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "runtime/psr-guzzle": "^0.1.0"
    },
    "require-dev": {
        "ely/php-code-style": "^1",
        "phpunit/phpunit": "^11",
        "roave/security-advisories": "dev-latest"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/erickskrauch/SkinRenderer2D.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ely\\SkinsRenderer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ely\\SkinsRenderer\\Tests\\": "tests"
        }
    }
}
